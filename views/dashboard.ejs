<html>
    <head>
        <title>Login</title>  
        <link href="/assets/css/dashboard.css" rel="stylesheet">
    </head>

    <body>
        <div class="wrapperFlex">
            <div class="wrapper">
                <h1>Welcome</h1>
                

                <div class="element">
                    <p><%= data.length%> Clicks in the last 24h</p>

                    <p><%= 100-Math.round((dataSorted.length / data.length) * 100) %>% clicked on a link</p>
                </div>

                <div>
                    <% data.forEach(function(element) { %>
                        <div class="element">
                            <table>
                                <tr>
                                    <td class="firstColomn">
                                        ID
                                    </td>
                                    <td>
                                        <%= element.log_id %>
                                    </td>
                                </tr>

                                <tr>
                                    <td>
                                        Time
                                    </td>
                                    <td>
                                        <%= element.log_dateTimeNew %>
                                    </td>
                                </tr>

                                <tr>
                                    <td>
                                        URL
                                    </td>
                                    <td>
                                        <%= element.log_url %>
                                    </td>
                                </tr>

                                <tr>
                                    <td>
                                        UUID
                                    </td>
                                    <td>
                                        <%= element.log_uuid %>
                                    </td>
                                </tr>
                            </table>
                            
                            
                            
                        </div>
                    <% }) %> 
                </div>
            </div>        
        </div>

        
    </body>
</html>